{
  "name": "rcon-console",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "preinstall": "yarn global add pkg",
    "build": "yarn build:linux && yarn build:win && yarn build:osx",
    "build:linux": "pkg -t node10-linux -o dist/goldsrc-rcon-console-lin main.js",
    "build:win": "pkg -t node10-win -o dist/goldsrc-rcon-console-win main.js",
    "build:osx": "pkg -t node10-macos -o dist/goldsrc-rcon-console-osx main.js"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "check-localhost": "^0.0.1",
    "commander": "^2.20.0",
    "ora": "^3.4.0",
    "stun": "^1.3.1",
    "vorpal": "^1.12.0"
  }
}
